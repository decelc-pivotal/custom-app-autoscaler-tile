SCHEME=https
ADMIN_USER=<%= Shellwords.escape properties.cf.admin_user %>
ADMIN_PASSWORD=<%= Shellwords.escape properties.cf.admin_password %>
DOMAIN=<%= Shellwords.escape properties.domain %>
APP_DOMAIN=<%= Shellwords.escape properties.app_domains[0] %>
CF_ORG=<%= Shellwords.escape properties.org %>
CF_ORG_QUOTA=<%= Shellwords.escape properties.org_quota %>
CF_SPACE=<%= Shellwords.escape properties.space %>
CF_TARGET=$SCHEME://api.${DOMAIN}
CF_SKIP_SSL=<%= Shellwords.escape properties.ssl.skip_cert_verify %>
SECURITY_USER_NAME=<%= Shellwords.escape properties.security.user %>
